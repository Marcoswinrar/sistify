{
  "name": "sistify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "pt": "ng serve --configuration=pt",
    "en": "ng serve --configuration=en",
    "build": "ng build",
    "build-pt": "ng build --prod --i18nFile src/i18n/messages.pt.xlf --i18nFormat xlf2 --i18nLocale pt",
    "build-en": "ng build --prod --i18nFile src/i18n/messages.en.xlf --i18nFormat xlf2 --i18nLocale en",
    "extract-i18n": "ng xi18n --output-path i18n --i18nFormat=xlf2 --i18nLocale en && xliffmerge --profile xliffmerge.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/language-service": "~7.2.15",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.2",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.17.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.2.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
